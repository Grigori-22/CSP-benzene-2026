Geometry = xyzFormat {
 <<<"start.xyz"
}

Hamiltonian = xtb {
  Method = "GFN2-xTB"
  SCCTolerance = 1.0E-8
  Charge =0
}

#Driver = GeometryOptimization {
#  Optimizer=Rational {}
#  MaxSteps=2000
#  OutputPrefix="geom.out"
#}

Options {
  WriteAutotestTag = No
  WriteResultsTag = Yes
}

Analysis {
  AtomResolvedEnergies = Yes
  CalculateForces = Yes
  MullikenAnalysis = Yes
  WriteNetCharges = Yes
}

ParserOptions { ParserVersion = 12 }
Parallel { UseOmpThreads = Yes }
